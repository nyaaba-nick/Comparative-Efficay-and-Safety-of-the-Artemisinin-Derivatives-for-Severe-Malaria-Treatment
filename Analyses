tiff("Fig24.tiff",
     width = 6000, height = 5000, res = 600, compression = "lzw")

hasse(net, c("Artemisinin;AMI", "Artemether;AME", "Arteether:ATE", "Artesunate;ASU","Quinine;QN"))

dev.off()





